{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "title1"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                10
            ]
        },
        "equity": {
            "type": "numeric",
            "default": "0.0",
            "title": "The equity Schema",
            "examples": [
                "0.1"
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [{
        "title": "title1",
        "salary": 10,
        "equity": "0.1",
        "company_handle": "c1"
    }]
}